<view style="background-color:#000000;"><block wx:for="{{video_list}}" wx:for-item="item" wx:for-index="index"><view class="flex"><video class="video vue-ref-in-for" src="{{item.video_url}}" preload="{{true}}" show-play-btn="false" controls="false" loop="true" id="{{'video_'+item.video_id}}" objectFit="fill" data-current="{{item.video_id}}" enable-progress-gesture="{{true}}" play-btn-position="center" poster="{{item.cover_url}}" data-ref="video_url" data-event-opts="{{[['tap',[['clickVideo',['$event']]]],['timeupdate',[['timeupdate',['$event']]]]]}}" bindtap="__e" bindtimeupdate="__e"></video><cover-view class="cover-view-left"><text class="view-left-text">{{"@"+item.nickname}}</text><view class="view-left-text-content"><text class="text-content-text">{{item.video_describe}}</text></view></cover-view><cover-view class="cover-view-right"><cover-image class="avater img" src="{{item.cover_url}}" data-event-opts="{{[['tap',[['tapAvater',['$event']]]]]}}" catchtap="__e"></cover-image><text class="right-follow">+</text><cover-image class="img-left" style="position:relative;top:-20rpx;" src="{{item.is_dianzan?'/static/video/axc.png':'/static/video/bed.png'}}" data-event-opts="{{[['tap',[['tapLove',['$event']]]]]}}" catchtap="__e"></cover-image><text class="right-text">{{item.dianzan}}</text><cover-image class="img-left" style="height:80rpx;" src="/static/video/ay2.png" data-event-opts="{{[['tap',[['tapMsg',['$event']]]]]}}" catchtap="__e"></cover-image><text class="right-text">{{item.pinglun}}</text><cover-image class="img-left" style="height:76rpx;" src="/static/video/b6p.png" data-event-opts="{{[['tap',[['tapShare',['$event']]]]]}}" catchtap="__e"></cover-image><text class="right-text">{{item.zhuanfa}}</text><cover-image class="musicIcon img" src="/static/video/changpian.png"></cover-image><cover-view class="progressBar" style="{{'width:'+(barWidth+'px')+';'}}" animation="{{animationData}}"></cover-view></cover-view></view></block></view>